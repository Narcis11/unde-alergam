    <script type="text/javascript">
	 var map;
      function initialize() {
        var mapOptions = {
          center: new google.maps.LatLng( 44.4325, 26.1039),
          zoom: 12

        };
       map = new google.maps.Map(document.getElementById("map-canvas"),
            mapOptions);
      
  	flightPath.setMap(map);
	}
   
      google.maps.event.addDomListener(window, 'load', initialize);    

function Validare()
{
    var name = document.ContactForm.cand;
    var email = document.ContactForm.unde;
   // var comment = document.ContactForm.Comment;

    if (cand.value == "")
    {
        window.alert("Te rugăm să completezi primul câmp.");
        cand.focus();
        return false;
    }
    
  if (unde.value == "")
    {
        window.alert("Te rugăm să completezi al doilea câmp.");
        unde.focus();
        return false;
    }
return true;
}
    </script>
